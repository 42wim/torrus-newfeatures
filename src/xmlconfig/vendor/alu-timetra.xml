        <param name="rrd-ds"            value="DropHPrOct" />
        <param name="comment"
           value="The number of high priority octets dropped by the Qchip" />
        <param name="graph-legend"      value="Bytes dropped" />
        <param name="vertical-label"    value="Bps" />
        <param name="precedence"        value="990" />
      </leaf>


      
